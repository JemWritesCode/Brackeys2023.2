GridBackground {
	--grid-background-color: #282828;
	--line-color: rgba(193, 196, 192, 0.1);
	--thick-line-color: rgba(193, 196, 192, 0.1);
	--spacing: 10;
}

.nameOfDialogueContainer
{
	margin-top: 3px;
	margin-left: 5px;
	padding-right: 5px;
}


.node #title{
	background-color: #923842;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	min-height: 36px;
}

#node-border{
	background-color: #383838;
}
#node-border{
	flex-shrink: 0;
}

.Label{
	margin-top: 5px;
	margin-left: 4px;
}
.TextBox{
	white-space: Normal;
}

#title-label {
	color: #ffffff;
}

.Colorfield
{
	width: 200px; 
}
.TextTime
{
	padding-bottom: 2px;
}
.TextDuration
{
	padding-bottom: 2px;
}

.unity-object-field
{
	padding-bottom: 2px;
}
.unity-enum-field
{
	padding-bottom: 2px;
}
#collapse-button
{
	height: 0px;
}

.node .ChoiceLabel{
	width: 75px;
	max-width: 75px;
}
.node .ChoiceLabel .unity-text-field__input{
	-unity-text-align: middle-right;
}

#Choice {
	max-width: 300px; 
	width: 300px; 
}
#Choice #output{
	width: 200px;
}
#Choice #output .port{
	max-width: 215px; 
	width: 215px;
}
#Choice #output .port .ChoiceLabel{
	max-width: 145px; 
	width: 145px;
}

#TimerChoice {
	max-width: 300px; 
	width: 300px; 
}
#TimerChoice #output{
	width: 200px;
}
#TimerChoice #output .port{
	max-width: 215px; 
	width: 215px;
}
#TimerChoice #output .port .ChoiceLabel{
	max-width: 145px; 
	width: 145px;
}
#TimerChoice #output .FirstPort .unity-label{
	margin-right: 18px;
}

#Event {
	max-width: 300px; 
	width: 300px; 
}

#Dialog {
	max-width: 200px; 
	width: 200px; 
}



#Event #node-border #title {
	background-color: #9649CB;
}

#Event #node-border #contents #top #output .port {
	--port-color: rgba(150, 73, 203, 1.0);
}

#Event #node-border #contents #top #input .port {
	--port-color: rgba(150, 73, 203, 1.0);
}

#Event .unity-toolbar-menu{
	background-color: #8742b7 !important;
	border-width: 0px !important;
	padding: 5px;
}

#Event .EventBox .unity-label{
	min-width: 75px !important;
}

#Event #unity-x-input .unity-label,
#Event #unity-y-input .unity-label,
#Event #unity-z-input .unity-label,
#Event #unity-w-input .unity-label{
	display: none;
}

.EventBox {
	flex-direction: row;
}

.EventSO {
	width: 260px;
	max-width: 260px;
	margin-left: 2px;
	margin-right: 0px;
	height: 20px;
}

.EventBtn{
	background-color: #3F3F3F !important;
	margin-top: 1px;
	margin-bottom: 1px;
}


#Start #node-border #title {
	background-color: #149A3E;
}

#Start #node-border #contents #top #output .port {
	--port-color: rgba(34, 153, 71, 1.0);
}

#Start #node-border #contents #top #input .port {
	--port-color: rgba(34, 153, 71, 1.0);
}

#Start .unity-base-field .unity-text-element{
	width: 25px;
	min-width: 25px;
}


#Dialog #node-border #title {
	background-color: #F9C22E;
}

#Dialog #node-border #title #title-label{
	color: #000000;
}

#Dialog #node-border #contents #top #output .port {
	--port-color: rgba(249, 194, 46, 1.0);
}

#Dialog #node-border #contents #top #input .port {
	--port-color: rgba(249, 194, 46, 1.0);
}


#Choice #node-border #title {
	background-color: #22AED1;
}

#Choice #node-border #title .unity-button {
	background-color: #1f9dbc;
	border-width: 0px;
}

#Choice #node-border #contents #top #output .port {
	--port-color: rgba(83, 179, 203, 1.0);
}

#Choice #node-border #contents #top #input .port {
	--port-color: rgba(83, 179, 203, 1.0);
}



#End #node-border #title {
	background-color: #A8323F;
}

#End #node-border #contents #top #output .port {
	--port-color: rgba(204, 61, 77, 1.0);
}

#End #node-border #contents #top #input .port {
	--port-color: rgba(204, 61, 77, 1.0);
}



#TimerChoice #node-border #title {
	background-color: #ff7700;
}

#TimerChoice #node-border #title .unity-button {
	background-color: #cc5f00;
	border-width: 0px;
}
#TimerChoice #node-border #title #title-label{
	color: #4c2400;
}
#TimerChoice #node-border #contents #top #output .port {
	--port-color: rgba(255, 119, 0, 1.0);
}
#TimerChoice #node-border #contents #top #input .port {
	--port-color: rgba(255, 119, 0, 1.0);
}
#TimerChoice .TextDuration {
	padding-bottom: 0px;
}

#Random #title{
	background-color: #515151;
}
#Random .port{
	--port-color: #515151;
}
#Random .output .unity-base-field{
	display: None !important;
	
}

#Command{
	width: 300px;
}
#Command #title{
	background-color: #14452f;
}
#Command #contents{
	display: None;
}
#Command #node-border{
	background-color: #0f5132;
}

#Command #unity-text-input{
	background-color: #0a5c36;
		border-left-width: 0px !important;
	border-right-width: 0px;
	border-top-width: 0px;
	border-bottom-width: 0px;
	white-space: Normal;
}


.port .unity-button{
	background-color: #3F3F3F !important;
}

